package project1;

import java.util.Random;
import java.util.Scanner;

public class strt {

	public static void main(String[] args) {
	// permutations of an array|
	
	
	int v[] = {1,2,3};
	permut(v,0);
	
		
		
	}
	public static void print_array(int[] a){
		for(int i = 0;i < a.length;i++) {
			System.out.print(a[i]);
		}
		System.out.println();
	}
	public static void swap(int[] a,int x,int y) {
		int d = a[x];
		a[x] = a[y];
		a[y] = d;
	}
	public static void permut(int a[],int x){
		//The logic is to fix an element of the array an then get the permutations of the rest of the array
		
		if(x == a.length-1) {
			print_array(a);
			return;
		}
		
		for(int i = x;i < a.length;i++) {
			swap(a,x,i);//swap the fixed element with the ith element of the array
			permut(a,x+1);
			swap(a,x,i);//reset the array for the next swaps
		}
		
		
	}

	
	}
	
